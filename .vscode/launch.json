    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Issue 100 to Downloads/Raspberry Pi magazines",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "-p",
                "~/Downloads/Raspberry Pi magazines",
                "-i",
                "100",
                "124",
                "136"
            ]
        },
        {
            "name": "Python Debugger: Download all to default directory",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal"
        },
    ]
}